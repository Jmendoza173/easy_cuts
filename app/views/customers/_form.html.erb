<%= form_for @customer do |f| %>
<%= f.label :name %>
<%= f.text_field :name %> <br> <br>

<%= f.label :age %>
<%= f.number_field :age %> <br> <br>

<%= f.label :location %>
<%= f.text_field :location %> <br> <br>

<%= f.label :email %>
<%= f.text_field :email %> <br> <br>

<%= f.label :password %>
<%= f.password_field :password %> <br> <br>

<%= f.label :cell %>
<%= f.number_field :cell %> <br> <br>

<%= f.submit%>


<% end %>